        end)
    end
    coroutine.wrap(ZVDY_fake_script)()
    local function LFRU_fake_script() -- TextButton_3.LocalScript 
        local script = Instance.new('LocalScript', TextButton_3)

        local AttackToggle = false
        script.Parent.MouseButton1Click:Connect(function()
            AttackToggle = not AttackToggle
            print(AttackToggle)
            if AttackToggle == true then
                script.Parent.BackgroundColor3 = Color3.new(0,255,0) script.Parent.Text = "on"
            else
                script.Parent.BackgroundColor3 = Color3.new(255,0,0) script.Parent.Text = "off"
            end
            while AttackToggle == true and wait() do
                local Players = game:GetService("Players")
                local LocalPlayer = Players.LocalPlayer

                local function GetClosest()
                    local Character = LocalPlayer.Character
                    local HumanoidRootPart = Character and Character:FindFirstChild("HumanoidRootPart")
                    if not (Character or HumanoidRootPart) then return end

                    local TargetDistance = math.huge
                    local Target

                    for i,v in ipairs(Players:GetPlayers()) do
                        if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                            local TargetHRP = v.Character.HumanoidRootPart
                            local mag = (HumanoidRootPart.Position - TargetHRP.Position).magnitude
                            if mag < TargetDistance then
                                TargetDistance = mag
                                Target = v
                            end
                        end
                    end

                    return tostring(Target)
                end
                -- Script generated by SimpleSpy - credits to exx#9394
                local autoatacktarget = GetClosest()
                local args = {
                    [1] = {
                        [1] = game:GetService("Players")[autoatacktarget].Character.LeftLowerLeg,
                        [2] = game:GetService("Players")[autoatacktarget].Character.LeftUpperArm,
                        [3] = game:GetService("Players")[autoatacktarget].Character.HumanoidRootPart.Part,
                        [4] = game:GetService("Players")[autoatacktarget].Character.HumanoidRootPart,
                        [5] = game:GetService("Players")[autoatacktarget].Character.LeftFoot,
                        [6] = game:GetService("Players")[autoatacktarget].Character.RightUpperArm,
                        [7] = game:GetService("Players")[autoatacktarget].Character.LowerTorso,
                        [8] = game:GetService("Players")[autoatacktarget].Character.Head,
                        [9] = game:GetService("Players")[autoatacktarget].Character.LeftLowerArm,
                        [10] = game:GetService("Players")[autoatacktarget].Character.LeftHand,
                        [11] = game:GetService("Players")[autoatacktarget].Character.RightUpperLeg,
                        [12] = game:GetService("Players")[autoatacktarget].Character.UpperTorso,
                        [13] = game:GetService("Players")[autoatacktarget].Character.LeftUpperLeg,
                        [14] = game:GetService("Players")[autoatacktarget].Character.RightFoot,
                        [15] = game:GetService("Players")[autoatacktarget].Character.RightLowerArm,
                        [16] = game:GetService("Players")[autoatacktarget].Character.RightLowerLeg,
                        [17] = game:GetService("Players")[autoatacktarget].Character.RightHand}}

                game:GetService("ReplicatedStorage").Events.SwingTool:FireServer(unpack(args))
            end
        end)
    end
    coroutine.wrap(LFRU_fake_script)()
end)

