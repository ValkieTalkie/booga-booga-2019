    local playerHead = game.Players.LocalPlayer.Character.HumanoidRootPart playerHead.CFrame = CFrame.new(-1052.59, -113.5, -669.416)
    wait(1.3)
    playerHead.CFrame = CFrame.new(-774.310059, 304.539398, -1210.83911, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    wait()
    playerHead.CFrame = CFrame.new()
    playerHead.CFrame = CFrame.new()
    playerHead.CFrame = CFrame.new()
end)
