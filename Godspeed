    -- Scripts:

    local function MWRRKD_fake_script() -- TextButton.LocalScript 
        local script = Instance.new('LocalScript', TextButton)

        getgenv().Toggled = false
        function spawnPart()
            local char = game.Players.LocalPlayer.Character

            char:WaitForChild("HumanoidRootPart")

            local partWeld = Instance.new("Weld")

            local reachPart = Instance.new("Part")
            reachPart.Transparency = 0
            reachPart.Anchored = false
            reachPart.CanCollide = false
            reachPart.Massless = true
            reachPart.Material = Enum.Material.ForceField
            reachPart.CFrame = char.PrimaryPart.CFrame * CFrame.new(0, 0, -reachPart.Size.Z / 2 + 1)
            reachPart.Color = Color3.fromRGB(139, 254, 255)
            reachPart.Parent = char
            reachPart.Size = Vector3.new(7, 13, 8)
            reachPart.Name = "RevampedReachPart"

            reachPart.Touched:Connect(function(part)
                if part.Parent ~= tostring(game.Players.LocalPlayer) and part.Name == "HumanoidRootPart" then
                    print(part,"STARTED")
                    reachPart.BrickColor = BrickColor.new("Really red")
                end
            end)

            reachPart.TouchEnded:Connect(function(part)
                if part.Parent ~= tostring(game.Players.LocalPlayer) and part.Name == "HumanoidRootPart" then
                    print(part,"ENDED")
                    reachPart.BrickColor = BrickColor.new("Pastel blue-green")
                end
            end)

            partWeld.Parent = reachPart
            partWeld.Part0 = reachPart
            partWeld.Part1 = char.PrimaryPart
            partWeld.C0 = CFrame.new(0, 0, reachPart.Size.Z / 2 - 1)
        end

        script.Parent.MouseButton1Click:Connect(function()
            if Toggled == false then
                Toggled = true
                spawnPart()
                script.Parent.BackgroundColor3 = Color3.new(0,255,0)
                script.Parent.Text = "on"
            else
                Toggled = false
                game.Players.LocalPlayer.Character.RevampedReachPart:Destroy()
                script.Parent.BackgroundColor3 = Color3.new(255,0,0)
                script.Parent.Text = "off"
            end
        end)

        game.Players.LocalPlayer.CharacterAdded:Connect(function(me)
            print(me)
            if me == game.Players.LocalPlayer.Character and Toggled == true then
                me:WaitForChild("HumanoidRootPart")
                spawnPart()
            end
        end)
    end
    coroutine.wrap(MWRRKD_fake_script)()
    local function ZVDY_fake_script() -- TextButton_2.LocalScript 
