local Tab = Window:NewTab("Teleports")
local TeleportsSection = Tab:NewSection("Teleports")

TeleportsSection:NewButton("Void Rock TP", "Void", function()
     local torso = game.Players.LocalPlayer.Character.HumanoidRootPart

for i,v in pairs(game:GetService'Workspace'.TeleportParts.Part:GetDescendants()) do
    if v.Name == "TouchInterest" and v.Parent then
        firetouchinterest(torso, v.Parent, 0)
        wait(0.1)
        firetouchinterest(torso, v.Parent, 1)
        break;
    end
end
end)


TeleportsSection:NewButton("Volcano", "Overworld", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1115.04919, -200.768265, -737.773132, 0.984812498, 0, 0.173621148, 0, 1, 0, -0.173621148, 0, 0.9848124)
end)

TeleportsSection:NewButton("Old God Hole", "Overworld", function()
